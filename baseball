bas <- baseball
table(bas$Year)
table(bas$Team)
names(bas)
length(table(bas$Year))
bas <- subset(baseball,Playoffs==1)
table(bas$Year)
table(table(bas$Year))
off <- table(bas$Year)
names(off)
str(names(off))
# c() function because the names of PlayoffTable are strings and not numbers
off[c("1990","2001")] 
bas$num <- off[as.character(bas$Year)]
(table(bas$num))
bas$world <- as.numeric(bas$RankPlayoffs==1)
table(bas$world)
names(bas)
bivar <- glm(world~Year,data = bas,family = "binomial")
summary(bivar)
bivar <- glm(world~RS,data = bas,family = "binomial")
summary(bivar)
bivar <- glm(world~RA,data = bas,family = "binomial")
summary(bivar)
bivar <- glm(world~W,data = bas,family = "binomial")
summary(bivar)
bivar <- glm(world~OBP,data = bas,family = "binomial")
summary(bivar)
bivar <- glm(world~SLG,data = bas,family = "binomial")
summary(bivar)
bivar <- glm(world~BA,data = bas,family = "binomial")
summary(bivar)
bivar <- glm(world~RankSeason,data = bas,family = "binomial")
summary(bivar)
bivar <- glm(world~OSLG,data = bas,family = "binomial")
summary(bivar)
bivar <- glm(world~num,data = bas,family = "binomial")
summary(bivar)
bivar <- glm(world~League,data = bas,family = "binomial")
summary(bivar)
mod <- glm(world~Year+RA+RankSeason+num,data = bas,family = "binomial")
summary(mod)
cor(bas[c("Year","RA","RankSeason","num")])
aic <- glm(world~Year,data = bas,family = "binomial")
summary(aic)
aic <- glm(world~RA,data = bas,family = "binomial")
summary(aic)
aic <- glm(world~RankSeason,data = bas,family = "binomial")
summary(aic)
aic <- glm(world~num,data = bas,family = "binomial")
summary(aic)
aic <- glm(world~Year+RA,data = bas,family = "binomial")
summary(aic)
aic <- glm(world~Year+RankSeason,data = bas,family = "binomial")
summary(aic)
aic <- glm(world~Year+num,data = bas,family = "binomial")
summary(aic)
aic <- glm(world~RA+RankSeason,data = bas,family = "binomial")
summary(aic)
aic <- glm(world~RA+num,data = bas,family = "binomial")
summary(aic)
aic <- glm(world~RankSeason+num,data = bas,family = "binomial")
summary(aic)
